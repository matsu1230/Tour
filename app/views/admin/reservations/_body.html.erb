<% if @trip %>
<table class="attr">
  <tr>
    <th>ツアー名</th>
    <td><%= @trip.tourname %></td>
  </tr>
  <tr>
    <th>ツアー内容</th>
    <td><%= @trip.tourcontent %></td>
  </tr>
</table>
<% end %>
<% if @detail %>
<table class="attr">
  <tr>
    <th>ツアー開始日</th>
    <td><%= @detail.start %></td>
  </tr>
  <tr>
    <th>ツアー終了日</th>
    <td><%= @detail.end %></td>
  </tr>
</table>
<% end %>

